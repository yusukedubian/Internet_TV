</div>

	<input type="hidden" name="contents_setting[local_url]" value=<%= request.env["HTTP_HOST"] %>>
	<input type="hidden" name="current_order[]"  id="list_items">

<script type="text/javascript" charset="utf-8">
function view_setting_type(str,no){
	var type = str;
	if (type == "audio_upload") {
		document.getElementById("audio_upload").style.display = "block";
		document.getElementById("audio_url").style.display = "none";
	}else{
		document.getElementById("audio_upload").style.display = "none";
		document.getElementById("audio_url").style.display = "block";
	}
}
</script>

<script type="text/javascript"> 
$(document).ready(function() { 
	$("#order_list").sortable({ 
		handle : '.handle', 
		revert: false,
		axis:'y',
		update : function () { 
		$('#list_items').val($('#order_list').sortable('toArray') );
    	}

		,
		stop: function(){
			var order_flag = false;
			var length = document.getElementById("music_length").value;
			var ordre = document.getElementById("list_items").value;
			var n = 0;
			var array = [];
			for (i = 1; i <= parseInt(length); i = i + 1) {
				if (parseInt(ordre.split(",")[n]) == i){
					order_flag = true;
				}
				else{
					order_flag = false;
					break
				}
				n += 1;

			}
			
			if (order_flag == false) {
				for (i = 1; i <= parseInt(length); i = i + 1) {
					var element_ump3 = document.getElementById(i + "_url_mp3");
					var element_uogg = document.getElementById(i + "_url_ogg");
					var element_uwav = document.getElementById(i + "_url_wav");
					var element_fmp3 = document.getElementById(i + "_file_mp3");
					var element_fogg = document.getElementById(i + "_file_ogg");
					var element_fwav = document.getElementById(i + "_file_wav");
					if (element_ump3 != null) {
						element_ump3.disabled = true;
					}
					if (element_uogg != null) {
						element_uogg.disabled = true;
					}
					if (element_uwav != null) {
						element_uwav.disabled = true;
					}
					if (element_fmp3 != null) {
						element_fmp3.disabled = true;
					}
					if (element_fogg != null) {
						element_fogg.disabled = true;
					}
					if (element_fwav != null) {
						element_fwav.disabled = true;
					}
				}
			}
			else{
				for (i = 1; i <= parseInt(length); i = i + 1) {
					var element_ump3 = document.getElementById(i + "_url_mp3");
					var element_uogg = document.getElementById(i + "_url_ogg");
					var element_uwav = document.getElementById(i + "_url_wav");
					var element_fmp3 = document.getElementById(i + "_file_mp3");
					var element_fogg = document.getElementById(i + "_file_ogg");
					var element_fwav = document.getElementById(i + "_file_wav");
					if (element_ump3 != null) {
						element_ump3.disabled = false;
					}
					if (element_uogg != null) {
						element_uogg.disabled = false;
					}
					if (element_uwav != null) {
						element_uwav.disabled = false;
					}
					if (element_fmp3 != null) {
						element_fmp3.disabled = false;
					}
					if (element_fogg != null) {
						element_fogg.disabled = false;
					}
					if (element_fwav != null) {
						element_fwav.disabled = false;
					}
				}
			}
		}

  	});
});
</script>

<script type="text/javascript">
	function box_obj(str,count){
		var flag = false;
		var checked_count = 0;
		var url_mp3 = document.getElementById(str+"_url_mp3");
		var url_ogg = document.getElementById(str+"_url_ogg");
		var url_wav = document.getElementById(str+"_url_wav");
		var file_mp3 = document.getElementById(str+"_file_mp3");
		var file_ogg = document.getElementById(str+"_file_ogg");
		var file_wav = document.getElementById(str+"_file_wav");
